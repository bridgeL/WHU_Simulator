{
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["DOM", "ESNext"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["src"]
}
